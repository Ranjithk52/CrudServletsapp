<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Register</servlet-name>
  	<display-name>Register</display-name>
  	<description></description>
  	<servlet-class>com.ojas.Register</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ShowuserServlet</servlet-name>
  	<display-name>ShowuserServlet</display-name>
  	<description></description>
  	<servlet-class>com.ojas.ShowuserServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Deleteuser</servlet-name>
  	<display-name>Deleteuser</display-name>
  	<description></description>
  	<servlet-class>com.ojas.Deleteuser</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditServlet</servlet-name>
  	<display-name>EditServlet</display-name>
  	<description></description>
  	<servlet-class>com.ojas.EditServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditUserServlet</servlet-name>
  	<display-name>EditUserServlet</display-name>
  	<description></description>
  	<servlet-class>com.ojas.EditUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Register</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ShowuserServlet</servlet-name>
  	<url-pattern>/showuser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Deleteuser</servlet-name>
  	<url-pattern>/deleteurl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditServlet</servlet-name>
  	<url-pattern>/editurl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditUserServlet</servlet-name>
  	<url-pattern>/edit</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  <welcome-file>home.html</welcome-file></welcome-file-list>
</web-app>
